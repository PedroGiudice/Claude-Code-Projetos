{
  "name": "monitoramento-oab",
  "description": "Monitoramento contínuo de publicações DJEN para OAB 129021/SP com classificação por cliente",
  "version": "1.0.0",
  "type": "daemon",
  "config": {
    "oab": {
      "numero": "129021",
      "uf": "SP"
    },
    "tribunais_prioritarios": [
      "TRT3", "TJMG", "TJSP", "TRT2", "TJRJ",
      "TJPR", "TRF3", "TRF4", "TST", "TJMA",
      "TRF1", "TRT8", "TRT5", "TRT15"
    ],
    "horarios_busca": ["09:00", "15:00"],
    "banco_dados": "E:/djen-data/oab-monitoring.db",
    "clientes_config": "E:/djen-data/clientes-config.json",
    "excel_output": "E:/djen-data/DJEN-{DATA}.xlsx"
  },
  "estimativas": {
    "publicacoes_dia": 25,
    "tempo_busca_minutos": 2,
    "buscas_por_dia": 2,
    "cobertura_percent": 95
  }
}
