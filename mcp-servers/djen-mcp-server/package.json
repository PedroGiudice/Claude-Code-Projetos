{
  "name": "djen-mcp-server",
  "version": "0.1.0",
  "description": "Servidor MCP para integração com API DJEN (Diário de Justiça Eletrônico Nacional) - Jurimetria e análise processual",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "djen",
    "juridico",
    "jurimetria",
    "pje",
    "cnj"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.7.0",
    "dotenv": "^16.4.7",
    "zod": "^3.24.1",
    "@xenova/transformers": "^2.17.2",
    "date-fns": "^4.1.0",
    "p-queue": "^8.0.1",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
