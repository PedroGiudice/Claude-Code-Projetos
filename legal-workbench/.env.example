# Legal Workbench Environment Variables
# Copy this file to .env and fill in your values

# Trello API Credentials (https://trello.com/app-key)
TRELLO_API_KEY=your_trello_api_key
TRELLO_API_TOKEN=your_trello_token

# Gemini API (for text-extractor OCR enhancement)
GEMINI_API_KEY=your_gemini_key

# Text Extractor Settings
MAX_CONCURRENT_JOBS=2
JOB_TIMEOUT_SECONDS=600

# Sentry Error Tracking (https://sentry.io)
# Create project at sentry.io -> Settings -> Client Keys (DSN)
SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx
ENVIRONMENT=development
